{
	"expo": {
		"name": "Finances4U",
		"slug": "Finances4U",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#107869"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#107869"
			},
			"backgroundColor": "#EDEEF1"
		},
		"plugins": [
			[
				"expo-image-picker",
				{
					"photosPermission": "O Finances4U precisa de permiss√£o para fazer envio de imagens para o perfil e para suas metas"
				}
			]
		]
	}
}
